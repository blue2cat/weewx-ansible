#weewx-ansible by Gavin S. 

#user to install weewx under
weewx_user: weewx

#current version of weewx 
weewx_version: 4.1.1


#use config file. If this option is not set, the current weewx config file will be used. 
use_config = false

#config file for weewx to use. 
weewx_config: weewx.conf

#specify the timezone
weewx_timezone: US/Seattle

#backup. set to true to backup the weewx config automatically 
backup: false


